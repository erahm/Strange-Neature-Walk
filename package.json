{
  "name": "strange-neature-walk",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "db": "docker-compose up -d",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "seed:docker": "docker-compose run --rm backend npm run seed"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
